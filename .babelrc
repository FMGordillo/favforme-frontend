{
  "presets": [
    "next/babel",
    ["@babel/preset-env", { "targets": { "node": "current" } }]
  ],
  "plugins": [
    [
      "styled-components",
      { "ssr": true, "displayName": true, "preprocess": false }
    ]
  ],
  "env": {
    "production": {
      "plugins": ["transform-remove-console"]
    }
  }
}
